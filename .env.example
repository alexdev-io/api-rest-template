# App
NODE_ENV=development
PORT=3000
HOST=0.0.0.0
API_VERSION=v1

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/api_db"

# Auth
JWT_ACCESS_SECRET=change-this-access-secret-min-16-chars
JWT_REFRESH_SECRET=change-this-refresh-secret-min-16-chars
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# CORS
CORS_ORIGIN=http://localhost:3000
